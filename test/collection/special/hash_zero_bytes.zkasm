        ; HASH ZERO BYTES

    $ => E              :MLOAD(lastHashPId)
    E + 1 => E          :MSTORE(lastHashPId)

    0 => HASHPOS

    0       :HASHPLEN(E)
    $       :HASHPDIGEST(E)

    $ => E              :MLOAD(lastHashKId)
    E + 1 => E          :MSTORE(lastHashKId)

    HASHPOS      :HASHKLEN(E)
    $            :HASHKDIGEST(E)

    $ => E              :MLOAD(lastHashSId)
    E + 1 => E          :MSTORE(lastHashSId)

    0            :HASHSLEN(E)
    $            :HASHSDIGEST(E)
