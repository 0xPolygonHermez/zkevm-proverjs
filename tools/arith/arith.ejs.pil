    selEq[7] * (eq12 + carry[0]) = selEq[7] * carry[0]' * 2**24;

    selEq[8] * (eq13 + carry[0]) = selEq[8] * carry[0]' * 2**24;
    selEq[8] * (eq14 + carry[1]) = selEq[8] * carry[1]' * 2**24;

    selEq[9] * (eq15 + carry[0]) = selEq[9] * carry[0]' * 2**24;
