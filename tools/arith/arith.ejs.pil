    // hrc1 in Global.BYTE2;
    // hrc2 in Global.BYTE2;
    // hrc3 in Global.BYTE2;
    // TODO
    { Global.CLK32[29] + Global.CLK32[30] + Global.CLK32[31], hrc2, hrc3} in {SEL_BYTE2_BIT21,Global.BYTE2, BYTE2_BIT21};
