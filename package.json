    "postblob_inner_compilecircom": ". ./pre.sh && cp -rf $BDIR/build/blob_inner.verifier_cpp/blob_inner.verifier.dat $BDIR/config/blob_inner/blob_inner.verifier.dat && cp -rf $BDIR/build/blob_inner.verifier_cpp $BDIR/c_files",
